{
  "name": "jardiscore/factory",
  "version": "1.0.0",
  "description": "The Factory class serves as a flexible instantiation and access factory for classes, supporting optional versioning and dependency injection (DI).",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Jardis Core Development",
      "email": "jardisCore@headgent.dev"
    }
  ],
  "require": {
    "php": ">=8.2",
    "psr/container": "^2.0",
    "jardispsr/factory": "^1.0",
    "jardispsr/classversion": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^2.0.4",
    "squizlabs/php_codesniffer": "^3.11.2"
  },
  "autoload": {
    "psr-4": {
      "JardisCore\\Factory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JardisCore\\Factory\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "echo '#!/usr/bin/env bash' > .git/hooks/pre-commit",
      "echo 'bash ./support/pre-commit-hook.sh' >> .git/hooks/pre-commit",
      "chmod +x .git/hooks/pre-commit"
    ]
  },
  "homepage": "https://github.com/jardiscore/factory",
  "support": {
    "issues": "https://github.com/jardiscore/factory/issues",
    "email": "jardisCore@headgent.dev"
  },
  "keywords": ["factory", "classversion", "loader", "domain driven design", "jardiscore", "jardis", "headgent"]
}
